
<div class="main-content">
    <main>
        <div class="recent-flex">
            <div class="project">
                <div class="card">
                    <div class="card-header ">
                        <h3>Admins</h3>

                        <div class="">
                            <div class="btn-group">
                                <button 
                                    class="btn btn-secondary dropdown-toggle" 
                                    type="button" 
                                    data-bs-toggle="dropdown" 
                                    aria-haspopup="true" 
                                    aria-expanded="false">
                                        Filter {{!-- <span class="las la-arrow-down"></span> --}}
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/dashboard-profiles/male-ad-filter">Male</a>
                                    <a class="dropdown-item" href="/dashboard-profiles/female-ad-filter">Female</a>
                                </div>
                            </div>
                            <div class="btn-group">
                                <button 
                                    class="btn btn-secondary dropdown-toggle" 
                                    type="button" 
                                    data-bs-toggle="dropdown" 
                                    aria-haspopup="true" 
                                    aria-expanded="false">
                                        Sort {{!-- <span class="las la-arrow-down"></span> --}}
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/dashboard-profiles/email-ad-sort">Email</a>
                                    <a class="dropdown-item" href="/dashboard-profiles/name-ad-sort">Name</a>
                                    <a class="dropdown-item" href="/dashboard-profiles">All</a>
                                </div>
                            </div>
                        </div>

                        
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table width="100%"> 
                                <thead> 
                                    <tr>    
                                        <td>Email </td>
                                        <td>Name  </td>
                                        <td>Gender </td>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each admin_profiles}}
                                        <tr>
                                            <td>
                                                <a href="/dashboard-profiles/{{this.slug}}" style="font-weight: bold;">{{this.email}}</a>
                                            </td>
                                            <td>{{this.name}}</td>
                                            <td>    
                                                {{this.gender}}
                                            </td>
                                        </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>

                <div class="card">
                    <div class="card-header ">
                        <h3>Customers</h3>
                        
                        <div class="">
                            <div class="btn-group">
                                <button 
                                    class="btn btn-secondary dropdown-toggle" 
                                    type="button" 
                                    data-bs-toggle="dropdown" 
                                    aria-haspopup="true" 
                                    aria-expanded="false">
                                        Filter {{!-- <span class="las la-arrow-down"></span> --}}
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/dashboard-profiles/male-cus-filter">Male</a>
                                    <a class="dropdown-item" href="/dashboard-profiles/female-cus-filter">Female</a>
                                    <a class="dropdown-item" href="/dashboard-profiles">All</a>
                                </div>
                            </div>
                            <div class="btn-group">
                                <button 
                                    class="btn btn-secondary dropdown-toggle" 
                                    type="button" 
                                    data-bs-toggle="dropdown" 
                                    aria-haspopup="true" 
                                    aria-expanded="false">
                                        Sort {{!-- <span class="las la-arrow-down"></span> --}}
                                </button>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="/dashboard-profiles/email-cus-sort">Email</a>
                                    <a class="dropdown-item" href="/dashboard-profiles/name-cus-sort">Name</a>

                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table width="100%"> 
                                <thead> 
                                    <tr>    
                                        <td>Email </td>
                                        <td>Name  </td>
                                        <td>Gender </td>
                                        <td>Option </td>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each customer_profiles}}
                                        <tr>
                                            <td>
                                                <a href="/dashboard-profiles/{{this.slug}}" style="font-weight: bold;">{{this.email}}</a>
                                            </td>
                                            <td>{{this.name}}</td>
                                            <td>    
                                                {{this.gender}}
                                            </td>
                                            <td>
                                                Ban
                                            </td>
                                        </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</div>