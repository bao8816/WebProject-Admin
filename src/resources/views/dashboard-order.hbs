
<div class="main-content">     
    <main>
        <div class="recent-flex">
            <div class="project">
                <div class="card">
                    <div class="card-header ">
                        <h3>All Order List Today</h3>
                        <button> See All <span class="las la-arrow-right"></span></button>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table width="100%"> 
                                <thead> 
                                    <tr>    
                                        <td>Customer </td>
                                        <td> Burger </td>
                                        <td>Price</td>
                                    </tr>
                                </thead>
                                <tbody>
                                   {{#each list_oders}}
                                    <tr>
                                        <td>{{this.name}}</td>
                                        <td>{{this.Burger}}</td>
                                        <td>    
                                            {{this.Price}}
                                        </td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
                
            </div>
        </div>
    </main>
</div>